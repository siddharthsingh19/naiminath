{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "resolvePackageJsonExports": true,
    "resolvePackageJsonImports": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowJs": true,
    "skipLibCheck": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/(components)/dropdown/Submenu.js",
    "src/app/(components)/dropdown/Item.js",
    "src/app/(components)/dropdown/Divider.js",
    "src/app/(components)/dropdown/index.js"
  ],
  "exclude": ["node_modules"]
}


